.sg-heatmap__heat-field,
.sg-heatmap__subgraph-picker {
    margin-right: 0;
}

.sg-heatmap__node-label {
    flex-grow: 1;
    -unity-text-align: middle-left;
    margin-left: 4px;
}

.sg-heatmap__list,
.sg-heatmap__help-box {
