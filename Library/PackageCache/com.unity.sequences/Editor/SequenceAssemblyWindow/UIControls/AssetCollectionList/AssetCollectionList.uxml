<ui:UXML xmlns:ui="UnityEngine.UIElements">
    <ui:VisualElement class="seq-list">
        <ui:VisualElement class="seq-header-text seq-list-header">
            <ui:Label text="Asset Collection" name="seq-asset-collection" />
        </ui:VisualElement>
        <ui:SelectableScrollView focusable="True" name="seq-sequence-asset-list" item-height="40" class="seq-list-content" />
        <ui:VisualElement name="seq-list-footer" class="seq-list-footer">
            <ui:VisualElement class="seq-list-footer-buttons">
                <ui:Button text="+" name="seq-sequence-asset-add" tooltip="Add a Sequence Asset" />
                <ui:Button text="-" name="seq-sequence-asset-remove" tooltip="Remove the selected Sequence Asset" />
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
