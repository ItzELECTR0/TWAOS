.seq-create-add-new-icon {
    background-image: resource("CreateAddNew.png");
}

.seq-dropdown-icon {
    background-image: resource("icon dropdown.png");
}

.seq-menu-button {
    background-image: resource("_Menu.png");
}

.seq-help-button {
    background-image: resource("_Help.png");
}

.seq-list-header {
    border-color: rgb(161, 161, 161);
}

.seq-list-content {
    background-color: rgb(200, 200, 200);
    border-color: rgb(161, 161, 161);
}

.seq-list-item:hover {
    background-color: rgb(186, 186, 186);
}

.seq-list-item--selected {
    background-color: rgb(58, 114, 176);
}

.seq-list-item--selected:hover {
    background-color: rgb(44, 93, 135);
}

.seq-list-footer-buttons {
    background-color: rgb(200, 200, 200);
    border-color: rgb(161, 161, 161);
}

.seq-list-footer-buttons Button:hover {
    background-color: rgb(192, 192, 192);
}

.seq-tree-view__item-label-invalid {
    color: rgb(142, 10, 10);
}

.unity-collection-view__item .seq-tree-view__item-label-not-loaded {
    color: gray;
}

.unity-collection-view__item.unity-collection-view__item--selected .seq-tree-view__item-label-not-loaded {
    color: lightgray;
}

