.seq-create-add-new-icon {
    background-image: resource("d_CreateAddNew.png");
}

.seq-dropdown-icon {
    background-image: resource("d_icon dropdown.png");
}

.seq-menu-button {
    background-image: resource("d__Menu.png");
}

.seq-menu-button:hover {
    background-color: rgb(65, 65, 65);
}

.seq-help-button {
    background-image: resource("d__Help.png");
}

.seq-list-header {
    border-color: rgb(36, 36, 36);
}

.seq-list-content {
    background-color: rgb(65, 65, 65);
    border-color: rgb(36, 36, 36);
}

.seq-list-item:hover {
    background-color: rgb(77, 77, 77);
}

.seq-list-item--selected {
    background-color: rgb(44, 93, 135);
}

.seq-list-item--selected:hover {
    background-color: rgb(58, 114, 176);
}

.seq-list-footer-buttons {
    background-color: rgb(65, 65, 65);
    border-color: rgb(36, 36, 36);
}

.seq-list-footer-buttons Button:hover {
    background-color: rgb(71, 71, 71);
}

.seq-tree-view__item-label-invalid {
    color: rgb(255, 120, 120);
}

.seq-tree-view__item-label-not-loaded {
    color: gray;
}

